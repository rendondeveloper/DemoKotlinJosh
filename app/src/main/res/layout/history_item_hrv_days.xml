<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="16dp"
    android:background="@drawable/shape_gray">

    <TextView
        android:id="@+id/tvActivity"
        style="@style/Kanit_Bold.Default.14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp"
        android:text="HRV VIERNES"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tvActivityDate"
        style="@style/Kanit_Regular.Default.12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:text="31/01/2023"
        android:textColor="@color/label_secondary"
        app:layout_constraintEnd_toEndOf="@+id/tvActivity"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tvActivity"
        app:layout_constraintTop_toBottomOf="@+id/tvActivity" />

    <TextView
        android:id="@+id/tvActivityDateOfficialDate"
        style="@style/Kanit_Bold_Italic.Default.24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:text="85%"
        app:layout_constraintBottom_toBottomOf="@+id/tvActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvActivity" />


    <ImageView
                android:contentDescription="@string/todo"
        android:id="@+id/imageView16"
        android:layout_width="14dp"
        android:layout_height="14dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/arrow_left"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvActivityDate" />

    <TextView
        android:id="@+id/tvActivityRitmo"
        style="@style/Kanit_Regular.Secondary.12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Del 20 al 26 de febrero "
        android:textColor="@color/label_secondary"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toStartOf="@+id/imageView17"
        app:layout_constraintStart_toEndOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="@+id/imageView16" />

    <ImageView
                android:contentDescription="@string/todo"
        android:id="@+id/imageView17"
        android:layout_width="14dp"
        android:layout_height="14dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="24dp"
        android:src="@drawable/arrow_right"
        app:layout_constraintBottom_toBottomOf="@+id/tvActivityRitmo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvActivityDate" />

    <TextView
        android:id="@+id/tvData1"
        style="@style/Kanit_Regular.Default.14"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="14dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/background_item_square_orange"
        android:paddingStart="6dp"
        android:paddingTop="4dp"
        android:paddingEnd="6dp"
        android:paddingBottom="4dp"
        android:text="75"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView16" />


    <TextView
        android:id="@+id/tvData2"
        style="@style/Kanit_Regular.Default.14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:backgroundTint="@color/red_view"
        android:background="@drawable/background_item_square_orange"
        android:paddingBottom="4dp"
        android:paddingTop="4dp"
        android:paddingEnd="6dp"
        android:paddingStart="6dp"
        android:text="72"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/tvData1"
        app:layout_constraintStart_toEndOf="@+id/tvData1"
        app:layout_constraintTop_toTopOf="@+id/tvData1"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tvData3"
        style="@style/Kanit_Regular.Default.14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:backgroundTint="@color/green_two_view"
        android:background="@drawable/background_item_square_orange"
        android:paddingBottom="4dp"
        android:paddingTop="4dp"
        android:paddingEnd="6dp"
        android:paddingStart="6dp"
        android:text="80"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/tvData2"
        app:layout_constraintStart_toEndOf="@+id/tvData2"
        app:layout_constraintTop_toTopOf="@+id/tvData2"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tvData4"
        style="@style/Kanit_Regular.Default.14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:backgroundTint="#FFC340"
        android:background="@drawable/background_item_square_orange"
        android:paddingBottom="4dp"
        android:paddingTop="4dp"
        android:paddingEnd="6dp"
        android:paddingStart="6dp"
        android:text="75"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/tvData3"
        app:layout_constraintStart_toEndOf="@+id/tvData3"
        app:layout_constraintTop_toTopOf="@+id/tvData3"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tvData5"
        style="@style/Kanit_Regular.Default.14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:paddingBottom="4dp"
        android:paddingTop="4dp"
        android:paddingEnd="6dp"
        android:paddingStart="6dp"
        android:text="-"
        app:layout_constraintBottom_toBottomOf="@+id/tvData4"
        app:layout_constraintStart_toEndOf="@+id/tvData4"
        app:layout_constraintTop_toTopOf="@+id/tvData4"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tvData6"
        style="@style/Kanit_Regular.Default.14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:paddingBottom="4dp"
        android:paddingTop="4dp"
        android:paddingEnd="6dp"
        android:paddingStart="6dp"
        android:text="-"
        app:layout_constraintBottom_toBottomOf="@+id/tvData5"
        app:layout_constraintStart_toEndOf="@+id/tvData5"
        app:layout_constraintTop_toTopOf="@+id/tvData5"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tvData7"
        style="@style/Kanit_Regular.Default.14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:paddingBottom="4dp"
        android:paddingTop="4dp"
        android:paddingEnd="6dp"
        android:paddingStart="6dp"
        android:text="-"
        app:layout_constraintBottom_toBottomOf="@+id/tvData6"
        app:layout_constraintStart_toEndOf="@+id/tvData6"
        app:layout_constraintTop_toTopOf="@+id/tvData6"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>