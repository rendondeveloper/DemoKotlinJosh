<resources>
    <declare-styleable name="BenefitsView">
        <attr name="benefit_title" format="string"/>
        <attr name="benefit_icon" format="string|reference"/>
        <attr name="benefit_description" format="string"/>
        <attr name="benefit_promotion" format="string"/>
        <attr name="benefit_style_image" format="enum">
            <enum name="small" value="0"/>
            <enum name="medium" value="1"/>
        </attr>
        <attr name="benefit_background" format="enum">
            <enum name="white" value="0"/>
            <enum name="dark" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="HistoryItem">
        <attr name="history_item_title" format="string"/>
        <attr name="history_item_calories" format="string"/>
        <attr name="history_item_average" format="string"/>
        <attr name="history_item_time" format="string"/>
    </declare-styleable>
    <declare-styleable name="HistoryItemTwo">
        <attr name="history_item_two_date" format="string"/>
        <attr name="history_item_two_score" format="string"/>
        <attr name="history_item_two_time" format="string"/>
    </declare-styleable>
    <declare-styleable name="DeviceItem">
        <attr name="device_item_title" format="string"/>
        <attr name="device_item_image" format="string|reference"/>
        <attr name="device_item_button_text" format="string"/>
        <attr name="device_item_description" format="string"/>
        <attr name="device_item_is_connect" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="SectionView">
        <attr name="section_title" format="string"/>
        <attr name="section_sub_title" format="string"/>
        <attr name="section_description" format="string"/>
        <attr name="section_footer" format="string|reference"/>
    </declare-styleable>
</resources>